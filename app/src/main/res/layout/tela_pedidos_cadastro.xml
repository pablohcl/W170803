<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_razao_social_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        style="@style/letras_pretas_negrito_24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/tv_fantasia_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        style="@style/letras_pretas_negrito_24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_razao_social_tela_cadastro_pedido" />

    <TextView
        android:id="@+id/tv_endereco_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        style="@style/letras_pretas_20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_fantasia_tela_cadastro_pedido" />

    <TextView
        android:id="@+id/tv_numero_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/tv_endereco_tela_cadastro_pedido"
        android:layout_marginLeft="8dp"
        style="@style/letras_pretas_20sp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_fantasia_tela_cadastro_pedido" />

    <TextView
        android:id="@+id/tv_bairro_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="8dp"
        style="@style/letras_pretas_20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_endereco_tela_cadastro_pedido" />

    <TextView
        android:id="@+id/tv_cidade_tela_cadastro_pedido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        style="@style/letras_pretas_20sp"
        android:text="TextView"
        app:layout_constraintLeft_toRightOf="@+id/tv_bairro_tela_cadastro_pedido"
        app:layout_constraintTop_toBottomOf="@+id/tv_numero_tela_cadastro_pedido" />
</android.support.constraint.ConstraintLayout>